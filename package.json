    "url": "https://github.com/Himbaza123h/My_Brand_Cohort8/issues"
  },
  "engines": {
    "node": "^12.14.1"
  },
  "homepage": "https://github.com/Himbaza123h/My_Brand_Cohort8#readme",
  "dependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@hapi/joi": "^17.1.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^5.0.1",
    "chalk": "^5.0.0",
