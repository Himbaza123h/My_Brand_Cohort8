    "mongoose": "^6.1.8",
    "streamifier": "^0.1.1",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@babel/register": "^7.16.9",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
